{
  "name": "share-menu",
  "version": "3.0.0",
  "description": "A complete and simple to use share menu.",
  "main": "share-menu.js",
  "module": "share-menu.js",
  "types": "share-menu.d.ts",
  "scripts": {
    "start": "run-p watch serve browser-sync",
    "watch": "tsc -w",
    "serve": "polymer serve --npm -p 8082",
    "browser-sync": "browser-sync start --proxy 'localhost:8082' --port 8080 --ui-port 8081 --files 'demo' 'share-menu.js'",
    "build": "tsc",
    "lint": "tslint -p .",
    "format": "tslint -p . --fix"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "browser-sync": "^2.26.0",
    "npm-run-all": "^4.1.3",
    "polymer-cli": "^1.8.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dabolus/share-menu.git"
  },
  "keywords": [],
